int Solution::solve(vector<int> &A, int B) {<br><br>
   int sum1=0,sum2=0,min1;<br><br>
   for(int i=0;i &lt; A.size();i++){sum1+=A[i];}<br><br>
   
   int i=0,j=A.size()-B-1;<br><br>
   
   for(int k=i;k &lt;=j;k++){sum2+=A[k];}<br><br>
   min1=sum2; i++; j++;<br><br>
   while(j &lt; A.size()){<br><br>
    &nbsp;&nbsp;  sum2+=(A[j]-A[i-1]);<br><br>
    &nbsp;&nbsp;  min1=min(sum2,min1);<br><br>
    &nbsp;&nbsp;  i++; j++;<br><br>
   }<br><br>
   &nbsp;return (sum1-min1);<br><br>
}<br><br>


